<Window x:Class="PCSU.UI.Windowing.OnDeleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
		Title="Remove Photos" Height="600" Width="400" ResizeMode="NoResize">
	<StackPanel Margin="10">
		<StackPanel>
			<ListBox Name="ListBox" SelectionMode="Extended" Height="400"/>
		</StackPanel>
		<StackPanel HorizontalAlignment="Right">
			<Button Name="ButtonRemove" Width="100" Margin="5" Content="Remove selected" Click="ButtonRemove_Click"/>
		</StackPanel>
		<StackPanel HorizontalAlignment="Right">
			<Button Name="ButtonCancel" Width="100" Margin="5" Content="Cancel" Click="ButtonCancel_Click"/>
		</StackPanel>
	</StackPanel>
</Window>